              })
              +card-faq({
                number: 3,
                parentId: 'accordion5'
              })
              +card-faq({
                number: 7,
                parentId: 'accordion5'
              })
              +card-faq({
                number: 6,
                parentId: 'accordion5'
              })
              +card-faq({
                number: 1,
                parentId: 'accordion5'
              })
              +card-faq({
                number: 5,
                parentId: 'accordion5'
              })

          div#tabs-faq-6.tab-pane.fade
            // Bootstrap Collapse
            #accordion6.card-group-custom.card-group-classic(role='tablist' aria-multiselectable='false')
              +card-faq({
                number: 5,
                parentId: 'accordion6',
                active: true
              })
              +card-faq({
                number: 2,
